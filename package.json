{
  "name": "tep-cms",
  "version": "0.0.1",
  "description": "This evergrowing, powerful content management system is perfect for your website",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"nodemon\" \"npm run watch-css\"",
    "watch-css": "sass --watch styles/styles.scss:public/css/styles.css --style=expanded",
    "prod": "npm run build && sass styles/styles.scss public/css/styles.css --no-source-map --style=compressed"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-handlebars": "^7.0.7",
    "express-session": "^1.18.2"
  },
  "author": "JirzyKerklaan",
  "license": "ISC",
  "devDependencies": {
    "@types/concurrently": "^6.4.0",
    "@types/express": "^5.0.3",
    "@types/express-handlebars": "^5.3.1",
    "@types/express-session": "^1.18.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.1.0",
    "@types/nodemon": "^1.19.6",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "sass": "^1.89.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
